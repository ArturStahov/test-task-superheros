 from 'gsap';\nimport { Bounce } from 'gsap';\nimport Header from './components/Header';\nimport Section from './components/Section';\nimport FormCreateHero from './components/FormCreateHero';\nimport HeroList from './components/HeroList';\nimport Modal from './components/Modal';\nimport HeroPreview from './components/HeroPreview';\nimport AuthTab from './components/Auth/AuthTab/AuthTab';\nimport ButtonLogOut from './components/ButtonLogOut';\nimport { AppBar } from './components/AppBar';\nimport { infoNotification } from './Notification/errorHandler';\nimport { userValue } from './selectors/authSelector';\nimport { getAllUserHero } from './redux/hero-card/hero-card-operations';\nimport { AllHeroNetwork } from './components/AllHeroNetwork';\nimport Logo from 'components/Header/Logo';\nimport ChatOpenButton from './components/Chat/ChatOpenButton';\nimport Chat from './components/Chat';\n\nexport default function App() {\n  const [itemPreview, setItemPreview] = useState(null);\n  const [toggleChat, setToggleChat] = useState(false);\n  const [chatRef, setChatRef] = useState(null);\n  const user = useSelector(userValue);\n  const dispatch = useDispatch();\n\n  useEffect(() => {\n    if (user) {\n      infoNotification('Welcome!');\n      dispatch(getAllUserHero(user));\n      console.log(user);\n      return;\n    }\n  }, [user, dispatch]);\n\n  const handlerOpenModal = item => {\n    setItemPreview(item);\n    console.log('itemPreviewHero', item)\n  };\n\n  const handlerCloseModal = () => {\n    setItemPreview(null);\n  };\n\n  const openChat = () => {\n    gsap.to(chatRef.current, {\n      duration: 3,\n      delay: 0,\n      x: 0,\n      rotateY: 0,\n      scale: 1,\n      ease: Bounce.easeOut,\n      opacity: 1,\n    });\n\n    // setToggleChat(true)\n  };\n  const closeChat = () => {\n    gsap.to(chatRef.current, {\n      duration: 3,\n      delay: 0,\n      x: -150,\n      scale: 0,\n      rotateY: 360,\n      opacity: 0,\n    });\n    // setToggleChat(false)\n  };\n  const getChatRef = chatRef => {\n    setChatRef(chatRef);\n  };\n\n  return (\n    <>\n      <Header>\n        {!user && <Logo title=\"hero factory\" />}\n\n        {user ? <FormCreateHero /> : <AuthTab />}\n        {user && <AppBar userName={user.nickName} userIco={user.photoURL} />}\n      </Header>\n      <main>\n        {user && <ButtonLogOut />}\n        {user && <ChatOpenButton onOpen={openChat} />}\n        {user && <Chat user={user} onClose={closeChat} getRef={getChatRef} />}\n        {user && (\n          <Section title=\"You Hero\">\n            <HeroList onPreviewItem={handlerOpenModal} />\n          </Section>\n        )}\n        {user && (\n          <Section title=\"All hero from network\">\n            {user && <AllHeroNetwork onPreviewItem={handlerOpenModal} />}\n          </Section>\n        )}\n\n        {itemPreview && (\n          <Modal onCloseModal={handlerCloseModal}>\n            <HeroPreview itemHero={itemPreview} />\n          </Modal>\n        )}\n      </main>\n    </>\n  );\n}\n","D:\\fs-online\\Practical_\\test-task-superheros\\src\\redux\\auth\\auth-reducer.js",[],"D:\\fs-online\\Practical_\\test-task-superheros\\src\\redux\\hero-card\\hero-card-reducer.js",[],"D:\\fs-online\\Practical_\\test-task-superheros\\src\\Notification\\errorHandler.js",[],"D:\\fs-online\\Practical_\\test-task-superheros\\src\\selectors\\authSelector.js",[],"D:\\fs-online\\Practical_\\test-task-superheros\\src\\redux\\hero-card\\hero-card-operations.js",[],"D:\\fs-online\\Practical_\\test-task-superheros\\src\\components\\Auth\\AuthTab\\AuthTab.js",[],"D:\\fs-online\\Practical_\\test-task-superheros\\src\\redux\\auth\\auth-action.js",[],"D:\\fs-online\\Practical_\\test-task-superheros\\src\\redux\\hero-card\\hero-card-action.js",[],"D:\\fs-online\\Practical_\\test-task-superheros\\src\\components\\Header\\index.js",[],"D:\\fs-online\\Practical_\\test-task-superheros\\src\\components\\HeroList\\index.js",[],"D:\\fs-online\\Practical_\\test-task-superheros\