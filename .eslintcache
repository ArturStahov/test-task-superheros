\n      text: formValue,\n      createdAt: db.firestore.FieldValue.serverTimestamp(),\n      id,\n      photoURL,\n      uniqKey: Date.now(),\n    });\n\n    setFormValue('');\n    dummy.current.scrollIntoView({ behavior: 'smooth' });\n  };\n\n  return (\n    <ChatModal ref={chatModalRef}>\n      <ChatCloseButton onClose={onClose} />\n      <ChatRoom>\n        {messageArr &&\n          messageArr.map(msg => (\n            <ChatMessage key={msg.uniqKey} message={msg} user={user} />\n          ))}\n\n        <span ref={dummy}></span>\n      </ChatRoom>\n\n      <Form onSubmit={sendMessage}>\n        <Input\n          value={formValue}\n          onChange={e => setFormValue(e.target.value)}\n          placeholder=\"input message\"\n          maxLength='50'\n        />\n\n        <Button\n          type=\"submit\"\n          disabled={!formValue}\n          variant=\"contained\"\n          color=\"primary\"\n          endIcon={<SendIcon />}\n        >\n          Send\n        </Button>\n      </Form>\n    </ChatModal>\n  );\n}\n","D:\\fs-online\\Practical_\\test-task-superheros\\src\\components\\AllHeroNetwork\\AllHeroNetwork.js",[],"D:\\fs-online\\Practical_\\test-task-superheros\\src\\components\\HeroList\\HeroList.js",[],"D:\\fs-online\\Practical_\\test-task-superheros\\src\\firebase\\config.js",[],"D:\\fs-online\\Practical_\\test-task-superheros\\src\\components\\Auth\\RegistrationForm\\StyledComponent.js",[],"D:\\fs-online\\Practical_\\test-task-superheros\\src\\components\\Auth\\AuthForm\\StyledComponent.js",[],"D:\\fs-online\\Practical_\\test-task-superheros\\src\\components\\Section\\StyledComponent.js",[],"D:\\fs-online\\Practical_\\test-task-superheros\\src\\components\\HeroPreview\\StyledComponent.js",[],"D:\\fs-online\\Practical_\\test-task-superheros\\src\\components\\FormCreateHero\\StyledComponent.js",[],"D:\\fs-online\\Practical_\\test-task-superheros\\src\\components\\ButtonLogOut\\StyledComponent.js",[],"D:\\fs-online\\Practical_\\test-task-superheros\